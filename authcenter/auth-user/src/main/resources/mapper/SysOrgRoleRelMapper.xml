<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cmiov.framework.sys.organ.mapper.SysOrgRoleRelMapper">
    <select id="findList" resultType="com.cmiov.framework.sys.organ.entity.SysOrgRoleRel">
        select * from sys_org_role_rel t
    </select>

    <!-- myself:批量插入 -->
    <insert id="insertOrgRole" >
        insert into sys_org_role_rel (org_id, role_id) values
        <foreach collection="roleIds" item="item" index="index" separator=",">
            (#{orgId},
            #{item})
        </foreach>
    </insert>
</mapper>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xzg.framework.sys.menu.mapper.SysMenuMapper">
    <select id="findAll" resultType="com.xzg.framework.sys.menu.entity.SysMenu">
        select id,name,menu_route,type,method,url from sys_menu t where delete_flag = 0
    </select>
</mapper>